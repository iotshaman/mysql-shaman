{
  "name": "mysql-shaman",
  "version": "1.0.2",
  "description": "Access MySql databases using a simple, familiar ORM syntax.",
  "main": "dist/index.js",
  "scripts": {
    "test": "nyc mocha",
    "build": "tsc",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "private": true,
  "bin": {
    "mysql-shaman": "./dist/cli/mysql-shaman-cli.js"
  },
  "typings": "public_api.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iotshaman/mysql-shaman.git"
  },
  "keywords": [
    "mysql",
    "node",
    "nodejs"
  ],
  "author": "Kyle Brown",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iotshaman/json-repo/issues"
  },
  "homepage": "https://github.com/iotshaman/mysql-shaman#readme",
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/mysql": "2.15.19",
    "@types/node": "^14.14.2",
    "@types/sinon": "10.0.2",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "mocha": "^9.1.0",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "fast-glob": "^3.2.7",
    "mysql": "^2.18.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/*.js",
      "coverage/*",
      "dist/*",
      "**/*.mock.ts"
    ],
    "reporter": [
      "html",
      "lcov"
    ],
    "all": true
  }
}
