{
  "name": "mysql-shaman",
  "version": "1.0.11",
  "description": "Access MySql databases using a simple, familiar ORM syntax.",
  "main": "dist/index.js",
  "scripts": {
    "test": "nyc mocha",
    "build": "tsc",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "private": true,
  "bin": {
    "mysql-shaman": "./dist/cli/mysql-shaman-cli.js"
  },
  "typings": "public_api.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iotshaman/mysql-shaman.git"
  },
  "keywords": [
    "mysql",
    "node",
    "nodejs"
  ],
  "author": "Kyle Brown",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iotshaman/json-repo/issues"
  },
  "homepage": "https://github.com/iotshaman/mysql-shaman#readme",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.6.0",
    "@types/sinon": "^10.0.13",
    "chai": "^4.3.7",
    "coveralls": "^3.1.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "sinon": "^15.0.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  },
  "dependencies": {
    "fast-glob": "~3.2.12",
    "mysql2": "^2.3.3",
    "password-generator": "^2.3.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/*.js",
      "coverage/*",
      "dist/*",
      "**/*.mock.ts"
    ],
    "reporter": [
      "html",
      "lcov"
    ],
    "all": true
  }
}
